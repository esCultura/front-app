{
  "name": "escultura",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/datetimepicker": "6.7.3",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "eas-cli": "^3.10.2",
    "expo": "~48.0.6",
    "expo-application": "~5.1.1",
    "expo-auth-session": "~4.0.3",
    "expo-crypto": "~12.2.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-location": "~15.1.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-web-browser": "~12.1.1",
    "i18next": "^22.4.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.2",
    "react-native": "0.71.6",
    "react-native-bootstrap-icons": "^1.5.0",
    "react-native-calendars": "^1.1294.0",
    "react-native-elements": "^3.4.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-image-crop-picker": "^0.39.0",
    "react-native-image-picker": "^5.3.1",
    "react-native-keychain": "^8.1.1",
    "react-native-maps": "1.3.2",
    "react-native-paper": "^5.4.1",
    "react-native-picker-select": "^8.0.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.11",
    "expo-localization": "~14.1.1",
    "expo-image-picker": "~14.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@testing-library/react-native": "^12.1.2",
    "jest": "^29.5.0"
  },
  "private": true,
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-navigation|@react-navigation|@react-native-community|react-native-vector-icons|@expo|expo)"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/Tests/**/*.test.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 40
      }
    }
  }
}
